pkgname=wine-mono
pkgver=4.6.3
pkgrel=0
pkgdesc="Wine's built-in replacement for Microsoft's .NET Framew"
url="http://wiki.winehq.org/Mono"
arch="any"
license="GPL"
depends="wine"
makedepends=""
install=""
subpackages=""
source="https://dl.winehq.org/wine/wine-mono/${pkgver}/wine-mono-${pkgver}.msi"

_builddir=
prepare() {
	mkdir -p "$srcdir"
}

build() {
	return 0
}

package() {
	mkdir -p "$pkgdir"/usr/share/wine/mono/
	cp "$srcdir"/wine-mono-${pkgver}.msi \
		"$pkgdir"/usr/share/wine/mono/
}

