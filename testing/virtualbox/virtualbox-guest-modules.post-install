#!/bin/sh

/sbin/depmod vboxsf
/sbin/depmod vboxvideo
/sbin/depmod vboxguest

/sbin/modprobe -s vboxsf
/sbin/modprobe -s vboxvideo
/sbin/modprobe -s vboxguest

echo "=============================================="
echo "Enable the virtualbox-guest-utils init script."
echo "=============================================="

exit 0
