#!/bin/sh

NORMAL="\033[1;0m"
STRONG="\033[1;1m"
GREEN="\033[1;32m"

print_strong() {
	local prompt="${STRONG}$1 ${GREEN}$2${NORMAL}"
        printf "${prompt} %s\n"
}

print_strong "\nTo configure inetutils-syslogd / BusyBox Syslog Services & Runlevels execute:" "\n\n/sbin/setup-inetutils-syslogd\n"
exit 0
