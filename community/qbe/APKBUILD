pkgname=qbe
pkgver=1.1
pkgrel=1
pkgdesc="A simple C compiler backend"
url="https://c9x.me/compile/"
arch="x86_64 aarch64 riscv64"
license="MIT"
source="$pkgname-$pkgver.tar.xz::https://c9x.me/compile/release/qbe-$pkgver.tar.xz"

build() {
	make PREFIX=/usr
}

check() {
	make check
}

package() {
	make PREFIX=/usr DESTDIR="$pkgdir" install
}

