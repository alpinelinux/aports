pkgname=xfce4
pkgver=4.20
pkgrel=0
pkgdesc="XFCE 4 meta package"
url="https://xfce.org"
arch="noarch"
license="GPL"
# https://mail.xfce.org/pipermail/xfce-announce/2024-December/001481.html
depends="
	exo>=$pkgver
	font-dejavu
	garcon>=0.8
	libxfce4ui>=$pkgver
	libxfce4util>=$pkgver
	libxfce4windowing>=$pkgver
	thunar>=$pkgver
	thunar-volman>=$pkgver
	tumbler>=$pkgver
	xfce4-appfinder>=$pkgver
	xfce4-panel>=$pkgver
	xfce4-power-manager>=$pkgver
	xfce4-session>=$pkgver
	xfce4-settings>=$pkgver
	xfconf>=$pkgver
	xfdesktop>=$pkgver
	xfwm4>=$pkgver
	"

package() {
	mkdir -p "$pkgdir"
}

