#!/bin/sh

ver_old="$2"

if [ "$(apk version -t "$ver_old" '1.21.2-r1')" = '<' ]; then
	cat >&2 <<-EOF
	*
	* libheif no longer provides AV1 (AVIF) encoder and HEVC (H.265) encoder by
	* default. If you use them, install libheif-aom and libheif-x265, respectively.
	*
	* libheif now installs dav1d instead of AOM as the default AV1 (AVIF) decoder.
	* 
	EOF
fi

exit 0
