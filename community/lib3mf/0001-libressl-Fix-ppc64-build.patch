From 12e09750af72afa362a26c68d3b9daf2fd3062f2 Mon Sep 17 00:00:00 2001
From: Jens Reidel <adrian@travitia.xyz>
Date: Wed, 17 Dec 2025 07:59:08 +0100
Subject: [PATCH] libressl: Fix ppc64 build

Signed-off-by: Jens Reidel <adrian@travitia.xyz>
---
 Libraries/libressl/CMakeLists.txt | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/Libraries/libressl/CMakeLists.txt b/Libraries/libressl/CMakeLists.txt
index 2f0dfa0e..217be986 100644
--- a/Libraries/libressl/CMakeLists.txt
+++ b/Libraries/libressl/CMakeLists.txt
@@ -351,7 +351,7 @@ elseif("${CMAKE_SYSTEM_PROCESSOR}" MATCHES "mips")
 elseif("${CMAKE_SYSTEM_PROCESSOR}" MATCHES "powerpc")
 	set(HOST_POWERPC true)
 elseif("${CMAKE_SYSTEM_PROCESSOR}" MATCHES "ppc64")
-	set(HOST_PPC64 true)
+	set(HOST_POWERPC64 true)
 elseif("${CMAKE_SYSTEM_PROCESSOR}" MATCHES "riscv64")
 	set(HOST_RISCV64 true)
 elseif("${CMAKE_SYSTEM_PROCESSOR}" MATCHES "sparc64")
