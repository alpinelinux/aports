Avoid installing headers and shared library into the py3-mupdf subpackage, as
we already install them in the mupdf-dev and mupdf-libs subpackage.

diff -upr mupdf-1.26.12.orig/Makefile mupdf-1.26.12/Makefile
--- mupdf-1.26.12.orig/Makefile	2025-12-19 18:10:25.440261565 +0100
+++ mupdf-1.26.12/Makefile	2025-12-19 18:10:44.465196444 +0100
@@ -645,7 +645,7 @@ ifneq ($(OS),OpenBSD)
 	ln -sf libmupdfcpp.$(SO)$(SO_VERSION) $(DESTDIR)$(libdir)/libmupdfcpp.$(SO)$(SO_VERSION_MAJOR)
 endif
 
-install-shared-python: install-shared-c++
+install-shared-python:
 	install -d $(DESTDIR)$(pydir)/mupdf
 	install -m $(SO_INSTALL_MODE) $(OUT)/_mupdf.$(SO) $(DESTDIR)$(pydir)/mupdf
 	install -m 644 $(OUT)/mupdf.py $(DESTDIR)$(pydir)/mupdf/__init__.py
