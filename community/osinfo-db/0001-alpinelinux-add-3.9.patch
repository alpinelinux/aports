+      <iso>
+        <volume-id>alpine-virt 3.9.\d x86_64$</volume-id>
+      </iso>
+    </media>
+
+    <!-- Xen Installer !-->
+    <media arch="x86_64">
+      <variant id="xen"/>
+      <iso>
+        <volume-id>alpine-xen 3.9.\d x86_64$</volume-id>
+      </iso>
+    </media>
