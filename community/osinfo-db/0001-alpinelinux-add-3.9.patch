+    </media>
+    <media arch="s390x">
+      <variant id="standard"/>
+      <iso>
+        <volume-id>alpine-std 3.9.\d s390x$</volume-id>
+      </iso>
+    </media>
+
+    <!-- Extended Installer !-->
+    <media arch="i686">
+      <variant id="extended"/>
+      <iso>
+        <volume-id>alpine-ext 3.9.\d x86$</volume-id>
