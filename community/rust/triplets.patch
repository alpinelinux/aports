+  },
+  "pre-link-args-crt": {
+    "gcc": [
+      "-nostdlib"
+    ]
+  },
+  "pre-link-objects-exe-crt": [
+    "crt1.o",
+    "crti.o"
+  ],
+  "relro-level": "full",
+  "target-c-int-width": "32",
+  "target-endian": "little",
+  "target-family": "unix",
+  "target-mcount": "\u0001_mcount",
+  "target-pointer-width": "64",
+  "vendor": "alpine",
+  "need-rpath": true
+}
