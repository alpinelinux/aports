+  "static-position-independent-executables": true,
+  "post-link-objects-crt": [
+    "crtn.o"
+  ],
+  "pre-link-args": {
+    "gcc": [
+      "-Wl,--as-needed",
+      "-Wl,-z,noexecstack",
+      "-Wl,--eh-frame-hdr"
