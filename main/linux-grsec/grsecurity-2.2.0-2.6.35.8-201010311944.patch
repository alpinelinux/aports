diff -urNp linux-2.6.35.8/include/asm-generic/atomic-long.h linux-2.6.35.8/include/asm-generic/atomic-long.h
--- linux-2.6.35.8/include/asm-generic/atomic-long.h	2010-08-26 19:47:12.000000000 -0400
+++ linux-2.6.35.8/include/asm-generic/atomic-long.h	2010-10-12 10:19:29.000000000 -0400
@@ -22,6 +22,12 @@
 
 typedef atomic64_t atomic_long_t;
 
+#ifdef CONFIG_PAX_REFCOUNT
