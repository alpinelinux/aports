diff -urNp linux-2.6.35.8/grsecurity/grsec_sock.c linux-2.6.35.8/grsecurity/grsec_sock.c
--- linux-2.6.35.8/grsecurity/grsec_sock.c	1969-12-31 19:00:00.000000000 -0500
+++ linux-2.6.35.8/grsecurity/grsec_sock.c	2010-09-17 20:12:37.000000000 -0400
@@ -0,0 +1,271 @@
+#include <linux/kernel.h>
+#include <linux/module.h>
+#include <linux/sched.h>
+#include <linux/file.h>
+#include <linux/net.h>
+#include <linux/in.h>
+#include <linux/ip.h>
