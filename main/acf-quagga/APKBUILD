pkgname=acf-quagga
pkgver=0.8.0
pkgrel=0
pkgdesc="A web-based system administration interface for quagga"
url="http://git.alpinelinux.org/cgit/acf-quagga"
arch="noarch"
license="GPL-2"
depends="acf-core lua quagga"
source="http://dev.alpinelinux.org/archive/$pkgname/$pkgname-$pkgver.tar.bz2"

package() {
	cd "$srcdir/$pkgname-$pkgver"
	make DESTDIR="$pkgdir" install
}
