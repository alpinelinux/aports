From ed8835383683d8f46233f8ec2f2056d720208e54 Mon Sep 17 00:00:00 2001
From: Leo <thinkabit.ukim@gmail.com>
Date: Mon, 29 Apr 2019 18:26:47 -0300
Subject: [PATCH] abuild.in: remove duplicate options_has call

---
 abuild.in | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/abuild.in b/abuild.in
index 7acc422..9edea57 100644
--- a/abuild.in
+++ b/abuild.in
@@ -77,7 +77,7 @@ want_check() {
 }
 
 default_cleanup_srcdir() {
-	if option_has options_has "chmod-clean" && test -d "$srcdir"; then
+	if options_has "chmod-clean" && test -d "$srcdir"; then
 		chmod -R +w "$srcdir"
 	fi
 	rm -rf "$srcdir"
