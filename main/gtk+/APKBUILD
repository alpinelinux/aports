pkgname=gtk+
pkgver=2.24
pkgrel=1
pkgdesc="metapackage for GTK+ runtime and development"
url=""
arch="all"
license="LGPL"
depends="gtk+2.0>=$pkgver"
makedepends="$depends_dev"
install=""
subpackages="$pkgname-dev"
url="https://www.gtk.org/"

dev() {
	depends="gtk+2.0-dev"
	mkdir -p "$subpkgdir"
}

package() {
	mkdir -p "$pkgdir"
}

