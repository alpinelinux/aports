       /* Bkwd rel or global. */
@@ -234,6 +239,9 @@ void dasm_put(Dst_DECL, int start, ...)
         n = 0;                  /* Start new chain for fwd rel if label exists. */
       goto linkrel;
     case DASM_REL_PC:
+      if (p[-2] >> 12 == 0xc) { /* RIL instruction needs 32-bit immediate. */
+        ofs += 2;
+      }
       pl = D->pclabels + n;
