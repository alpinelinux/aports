+  |  lgr RB, BASE			// Save BASE. // TODO: needed?
+  |  lgfr CARG2, RA
+  |  brasl r14, extern lj_tab_getinth	// (GCtab *t, int32_t key)
+  |  // cTValue * or NULL returned in r2 (CRET1).
+  |  lgr BASE, RB
+  |  ltgr RD, CRET1
+  |  jne <1
   |->fff_res0:
-  |  stg r0, 0(r0)
-  |  stg r0, 0(r0)
+  |  lghi RD, 1+0
+  |  j ->fff_res
   |
   |.ffunc_1 ipairs
