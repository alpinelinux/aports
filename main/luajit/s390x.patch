       ofs += 2;
       break;
     case DASM_REL_EXT:
+      p++;
+      ofs += 4;
       break;
     case DASM_ALIGN:
       ofs += *p++;
       b[pos++] = ofs;
       break;
     case DASM_REL_LG:
+      if (p[-2] >> 12 == 0xc) { /* RIL instruction needs 32-bit immediate. */
