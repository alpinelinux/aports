-|  sllg TMPR1, TMPR1, 3(r0) // TMPR1=OP*8
+|  llgc OP, 3(PC)
+|  llgh RD, 0(PC)
+|  llgc RA, 2(PC)
+|  sllg TMPR1, OP, 3(r0)
 |  lg TMPR1, 0(TMPR1, DISPATCH)
+|  la PC, 4(PC)
 |  br TMPR1
 |.endmacro
