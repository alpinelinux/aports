pkgname=acf-freeswitch-vmail
pkgver=0.2.1
pkgrel=0
pkgdesc="ACF for a web interface for freeswitch voicemail"
url="http://git.alpinelinux.org/cgit/$pkgname"
arch="noarch"
license="GPL-2"
depends="acf-core lua lua-sql-sqlite3 freeswitch"
makedepends=""
install="$pkgname.post-upgrade"
subpackages=""
source="http://dev.alpinelinux.org/archive/$pkgname/$pkgname-$pkgver.tar.bz2"

package() {
	cd "$srcdir"/$pkgname-$pkgver
	make DESTDIR="$pkgdir" install
}

