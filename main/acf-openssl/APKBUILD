pkgname=acf-openssl
pkgver=0.10.1
pkgrel=3
pkgdesc="A web-based system administration interface for open/librelssl"
url="https://git.alpinelinux.org/cgit/acf/acf-openssl"
arch="noarch"
license="GPL-2.0"
depends="acf-core libressl"
makedepends=""
