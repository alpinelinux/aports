See https://github.com/antirez/redis/pull/3494

--- a/src/Makefile
+++ b/src/Makefile
@@ -307,9 +307,9 @@
 	$(REDIS_INSTALL) $(REDIS_SERVER_NAME) $(INSTALL_BIN)
