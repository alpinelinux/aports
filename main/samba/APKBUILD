builddir="$srcdir/$pkgname-$pkgver"

# secfixes:
#   4.7.6-r1:
#     - CVE-2018-1139
#     - CVE-2018-10858
#     - CVE-2018-10918
#     - CVE-2018-10919
#   4.7.6-r0:
#     - CVE-2018-1050
#     - CVE-2018-1057
#   4.7.3-r0:
#     - CVE-2017-14746
#     - CVE-2017-15275
#   4.7.0-r0:
