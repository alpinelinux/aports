pkgname=acf-samba
pkgver=0.10.0
pkgrel=2
pkgdesc="A web-based system administration interface for samba"
url="https://git.alpinelinux.org/cgit/acf/acf-samba"
arch="noarch"
license="GPL-2.0"
depends="acf-core samba"
