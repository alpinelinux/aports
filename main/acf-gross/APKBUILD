pkgname=acf-gross
pkgver=0.3.1
pkgrel=0
pkgdesc="A web-based system administration interface for gross"
url="http://git.alpinelinux.org/cgit/acf-gross"
arch="noarch"
license="GPL-2"
depends="acf-core lua gross"
source="http://dev.alpinelinux.org/archive/$pkgname/$pkgname-$pkgver.tar.bz2"

package() {
	cd "$srcdir/$pkgname-$pkgver"
	make DESTDIR="$pkgdir" install
}
